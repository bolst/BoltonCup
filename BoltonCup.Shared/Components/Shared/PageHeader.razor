
@if (!string.IsNullOrEmpty(Title))
{
    <PageTitle>@Title</PageTitle>
}

<MudText Class="ml-6 mt-20" Typo="Typo.h4"><b>@ChildContent</b></MudText>
<MudDivider />

@code{

    [Parameter]
    public RenderFragment ChildContent { get; set; }

    [Parameter] 
    public string? Title { get; set; }
    
}